package constructor;

public class CarUser {

	public static void main(String[] args) {
		
		Car car1 = new Car();
		
		car1.model= "Maruti";
		car1.speed= 120;
		car1.type= "Petrol";
		car1.wheels= 4;
		
		car1.features();
		
		
		Car car2 = new Car();
		
		car2.model= "Ford";
		car2.speed= 150;
		car2.type= "Diesel";
		car2.wheels= 4;
		
		car2.features();
	}

}
