package conditionalStatements;

public class MathameticalOperators {

	public static void main(String[] args) {
		int a= 10;
		int b = 4;
		double sum;
		
		System.out.println(a+b);
		System.out.println(b-a);
		System.out.println(a/b);
		System.out.println(a*b);
		System.out.println(sum = a%b);
		
	}

}
